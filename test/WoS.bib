@article{ ISI:000321262500038,
Author = {Melentiev, Pavel N. and Afanasiev, Anton E. and Kuzin, Artur A. and
   Baturin, Andrey S. and Balykin, Victor I.},
Title = {{Subwavelength light localization based on optical nonlinearity and light
   polarization}},
Journal = {{OPTICS LETTERS}},
Year = {{2013}},
Volume = {{38}},
Number = {{13}},
Pages = {{2274-2276}},
Month = {{JUL 1}},
Abstract = {{We propose and experimentally realize subwavelength light localization
   based on the optical nonlinearity of a single nonlinear element in
   nanoplasmonics-a split hole resonator (SHR). The SHR is composed of two
   basic elements of nanoplasmonics, a nanohole, and a nanorod. A peak
   field intensity occurs at the single spot of the SHR nanostructure. We
   demonstrate the use of the SHR as a highly efficient nonlinear optical
   element for (i) the construction of a polarization-ultrasensitive
   nanoelement and, as a practical application, (ii) the building up of an
   all-optical display. (C) 2013 Optical Society of America}},
